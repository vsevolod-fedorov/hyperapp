repo_list_storage = record:
  path_list: string list


repo_list_model = record

repo_item = record:
  name: string
  path: string
  current_branch: string


ref_list_model = record:
  repo_name: string
  repo_dir: string

ref_item = record:
  name: string
  commit_id_short: string
  commit_author: string
  commit_dt: datetime
  commit: ref


commit = record:
  id: string
  short_id: string
  parents: ref list
  time: datetime
  author: string
  committer: string
  message: string
