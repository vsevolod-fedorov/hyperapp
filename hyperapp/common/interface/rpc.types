
endpoint = record:
  peer_ref: ref
  iface_ref: ref
  object_id: string


request = record:
  sender_peer_ref: ref
  iface_ref: ref
  object_id: string
  request_id: string
  method_name: string
  params_ref: ref

response = record:
  request_id: string
  result_ref: ref

error_response = record:
  request_id: string
  exception_ref: ref
