
resource = hierarchy resource

resource_id = string list

resource_rec = record:
    id: resource_id
    resource: resource

resource_rec_list = resource_rec list


command_resource = resource class command:
    is_default: bool
    text: string opt
    description: string opt
    shortcuts: string list

column_resource = resource class column:
    visible: bool
    text: string opt
    description: string opt

error_message_resource = resource class error_message:
    message: string
