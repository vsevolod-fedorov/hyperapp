from error import error, client_error


test_error = error class test(client_error):
    error_message: string


packet = record:
    message: string


echo = interface echo:
    commands:
        say (message: string) -> (response: string)
        eat (message: string) -> ()
        notify (message: string)
        fail (message: string) -> ()
        subscribe (service_ref: ref) -> ()
        broadcast (message: string)


echo_notification_iface = interface echo_notification:
    commands:
        notify (message: string)
