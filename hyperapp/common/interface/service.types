
service = record:
  peer_ref: ref
  servant_path: ref list
  dir_list: ref list list  # Additional to own service type (list/tree/record) dirs.
  command_ref_list: ref list


column = record:
  id: string
  type_ref: ref

multi_item_service = record(service):
  key_column_id: string
  column_list: column list


list_service = record(multi_item_service)
tree_service = record(multi_item_service)

record_service = record(service)
