
param = record:
  name: string
  value: ref

request = record:
  request_id: string
  servant_ref: ref
  params: param list
  sender_peer_ref: ref

response = record:
  request_id: string
  result_ref: ref

error_response = record:
  request_id: string
  exception_ref: ref

server_error = exception:
  message: string
  traceback: string list
