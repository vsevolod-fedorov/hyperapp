
service = record:
  identity: ref
  function: ref
  dir: ref  # Additional to own service type (list/tree/record) dirs.
  commands: ref list

multi_item_service = record(service):
  key_attribute: string

list_service = record(multi_item_service)
tree_service = record(multi_item_service)

live_list_service = record(list_service)

record_service = record(service)
