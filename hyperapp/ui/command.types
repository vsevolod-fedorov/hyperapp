typed_config_ctl = record
untyped_config_ctl = record

cfg_item = record:
  t: ref
  command: ref

properties = record:
  is_global: bool
  uses_state: bool
  remotable: bool

fn_command = record:
  d: ref
  properties: properties
  system_fn: ref

ui_command = record(fn_command)
model_command = record(fn_command)

model_command_enumerator = record:
  function: ref
  params: string list
