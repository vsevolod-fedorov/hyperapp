config_ctl = record

properties = record:
  is_global: bool
  uses_state: bool
  remotable: bool

fn_command = record:
  d: ref
  properties: properties
  function: ref
  ctx_params: string list
  service_params: string list

ui_command = record(fn_command)
model_command = record(fn_command)

model_command_enumerator = record:
  function: ref
  params: string list
