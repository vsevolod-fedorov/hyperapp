from build import python_module_src


job = record:
  python_module: python_module_src
  idx: int
  resources: ref list
  test_fn_name: string

used_imports = record:
  module_name: string
  imports: string list list

succeeded_result = record:
  used_imports: used_imports list
  requirements: ref list
  constructors: ref list

incomplete_result = record:
  used_imports: used_imports list
  requirements: ref list
  error: string
  traceback: string list

failed_result = record:
  error: string
  traceback: string list


unknown_service_error = exception:
  service_name: string

config_item_missing_error = exception:
  service_name: string
  t: ref  # Assume we get only type-keyed errors.
