from python_src import python_module_src
from system_job import req_to_resource


job = record:
  python_module: python_module_src
  idx: int
  req_to_resource: req_to_resource list
  test_fn_name: string

used_imports = record:
  module_name: string
  imports: string list list

succeeded_result = record:
  used_requirements: ref list
  used_imports: used_imports list
  constructors: ref list

incomplete_result = record:
  missing_requirements: ref list
  used_requirements: ref list
  used_imports: used_imports list
  error: string
  traceback: string list

failed_result = record:
  used_requirements: ref list
  error: string
  traceback: string list
